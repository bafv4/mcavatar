{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "functions": {
    "api/avatar.ts": {
      "memory": 1024,
      "maxDuration": 10
    },
    "api/fullbody.ts": {
      "memory": 2048,
      "maxDuration": 30
    }
  },
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "key": "Access-Control-Allow-Methods",
          "value": "GET, OPTIONS"
        }
      ]
    }
  ]
}
